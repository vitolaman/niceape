{
  "name": "nice-ape",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "config:check": "node scripts/check-key-network.js",
    "config:generate": "node scripts/generate-devnet-config.js",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "worker:dev": "wrangler dev worker/index.ts --remote",
    "worker:deploy": "wrangler deploy worker/index.ts",
    "worker:deploy:prod": "wrangler deploy worker/index.ts --env production",
    "worker:tail": "wrangler tail",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply nice-ape-dev --remote",
    "db:migrate:prod": "wrangler d1 migrations apply nice-ape-prod --env production --remote",
    "db:studio": "drizzle-kit studio",
    "db:query": "wrangler d1 execute nice-ape-dev --remote --command",
    "db:create:dev": "wrangler d1 create nice-ape-dev",
    "db:create:prod": "wrangler d1 create nice-ape-prod"
  },
  "dependencies": {
    "@cloudflare/next-on-pages": "^1.13.12",
    "@cloudflare/workers-types": "^4.20250712.0",
    "@coral-xyz/anchor": "^0.31.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@iconify-json/ic": "^1.2.2",
    "@iconify-json/ph": "^1.2.2",
    "@iconify-json/solar": "^1.2.2",
    "@iconify/tailwind": "^1.2.0",
    "@jup-ag/wallet-adapter": "^0.2.3",
    "@meteora-ag/dynamic-bonding-curve-sdk": "^1.3.0",
    "@mui/material": "^7.2.0",
    "@mui/x-charts": "^8.8.0",
    "@solana/spl-token": "^0.4.13",
    "@solana/spl-token-registry": "^0.2.4574",
    "@solana/wallet-adapter-backpack": "^0.1.14",
    "@solana/wallet-adapter-wallets": "^0.19.33",
    "@solana/web3.js": "^1.95.1",
    "@tailwindcss/cli": "^4.1.4",
    "@tanstack/react-form": "^1.2.3",
    "@tanstack/react-query": "4.29.5",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.8",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "aws-sdk": "^2.1692.0",
    "bn.js": "^5.2.1",
    "bs58": "^6.0.0",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "decimal.js": "^10.5.0",
    "dotenv": "^17.0.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.8.2",
    "formidable": "^3.5.4",
    "jotai": "^2.12.3",
    "ky": "^1.8.1",
    "next": "^15.3.4",
    "next-auth": "^4.24.11",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "radix-ui": "~1.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-use": "^17.6.0",
    "sonner": "^2.0.2",
    "superstruct": "^2.0.2",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "twitter-api-v2": "^1.24.0",
    "wrangler": "^4.24.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/formidable": "^3.4.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "^10.1.2",
    "form-data": "^4.0.3",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "3.3.1",
    "typescript": "^5"
  }
}
